  "devDependencies": {
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.73",
    "autoprefixer": "^10.4.19",
    "dotenv": "^17.2.2",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "glob": "^10.4.5",
    "postcss": "^8.4.38",
    "svgo": "^3.3.2",
    "typescript": "^5.4.3"
  },
