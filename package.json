{
  "name": "corner-house",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS=\"--no-deprecation\" next dev",
    "build": "NODE_OPTIONS=\"--no-deprecation\" next build",
    "postbuild": "NODE_OPTIONS=\"--no-deprecation\" next-sitemap",
    "build:clean": "npm run clean && npm run build",
    "clean": "rm -rf .next",
    "start": "next start",
    "lint": "next lint",
    "colors:check": "node scripts/generate-color-css.js --check",
    "colors:generate": "node scripts/generate-color-css.js",
    "prebuild": "npm run colors:generate",
    "content:validate": "node scripts/validate-content.js validate",
    "content:build": "node scripts/build-content.js",
    "images:optimize": "node scripts/image-optimizer.js all"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@headlessui/react": "^1.7.18",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.6.8",
    "caniuse-lite": "^1.0.30001731",
    "client-only": "^0.0.1",
    "clsx": "^2.1.1",
    "daisyui": "4.10.1",
    "framer-motion": "^12.23.12",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.526.0",
    "next": "^14.2.30",
    "next-auth": "^4.24.11",
    "next-sitemap": "^4.2.3",
    "nextjs-toploader": "^1.6.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-tooltip": "^5.26.3",
    "server-only": "^0.0.1",
    "swr": "^2.3.6",
