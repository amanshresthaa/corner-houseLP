{
  "env": "prod",
  "featureFlags": {
    "cms": true,
    "posSync": true,
    "debug": false,
    "analytics": true,
    "environmentBanner": false
  },
  "api": {
    "baseUrl": "https://{{brand.domain}}",
    "menuEndpoint": "https://{{brand.domain}}/api/menu",
    "marketingEndpoint": "https://{{brand.domain}}/api/marketing",
    "restaurantEndpoint": "https://{{brand.domain}}/api/restaurant",
    "contentEndpoint": "https://{{brand.domain}}/api/content"
  },
  "cms": { 
    "enabled": true,
    "source": "api",
    "fallback": "filesystem"
  },
  "metadata": {
    "appName": "{{brand.fullName}}",
    "domainName": "{{brand.domain}}",
    "environment": "production"
  },
  "performance": {
    "caching": {
      "content": 3600,
      "menu": 7200,
      "static": 86400
    }
  }
}
