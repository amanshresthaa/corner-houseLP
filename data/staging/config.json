{
  "env": "staging",
  "featureFlags": {
    "cms": true,
    "posSync": false,
    "debug": false,
    "analytics": true,
    "environmentBanner": true
  },
  "api": {
    "baseUrl": "https://staging.{{brand.domain}}",
    "menuEndpoint": "https://staging.{{brand.domain}}/api/menu",
    "marketingEndpoint": "https://staging.{{brand.domain}}/api/marketing",
    "restaurantEndpoint": "https://staging.{{brand.domain}}/api/restaurant",
    "contentEndpoint": "https://staging.{{brand.domain}}/api/content"
  },
  "cms": { 
    "enabled": true,
    "source": "api",
    "fallback": "filesystem"
  },
  "metadata": {
    "appName": "{{brand.shortName}} [STAGING]",
    "domainName": "staging.{{brand.domain}}",
    "environment": "staging"
  },
  "performance": {
    "caching": {
      "content": 300,
      "menu": 600,
      "static": 7200
    }
  }
}
